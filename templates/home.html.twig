{% extends 'base.html.twig' %}

{% block title %}Accueil{% endblock %}

{% block body %} 
<div>
    <section class="jumbotron text-center">
	    <div class="row justify-content-center">
                        <div class="col-12 col-md-10 col-lg-8">
                            <form class="card card-sm" action="{{ path('search') }}"  method="POST">
                                <div class="card-body row no-gutters align-items-center">
                                    <div class="col-auto">
                                        <i class="fas fa-search h4 text-body"></i>
                                    </div>
                                    <!--end of col-->
                                    <div class="col">
                                        <input class="form-control form-control-lg form-control-borderless" name="city" 
                                        type="text" placeholder="Quelle ville ?">
                                    </div>
                                    <!--end of col-->
                                    <div class="col-auto">
                                        <button type="submit" class="btn btn-lg btn-success" role="button">Rechercher</button>
                                    </div>
                                    <!--end of col-->
                                </div>
                               <div class="row">
                                    <div class="col-md-6">
                                        <label>Date début</label>
                                        <input type="date" name="dated" id="debut" width="270">
                                    </div>
                                    <div class="col-md-6">
                                        <label>Date fin</label>
                                        <input type="date" name="datef" id="fin">
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!--end of col-->
                    </div>
    </section>
</div>

<div class="container" mt>
       <h2> Dernières voitures</h2>
       <div class="row flex">
        {% for car in cars %}
            <div class="col-3">
              <div class="card">
                  <div class="card-body">
                   <div class="img"><img src="data:image/jpeg;base64," alt="..." class="img-thumbnail"></div>
                   <h5 class="card-title">
                     <a href="{{ path('car_detail', {'id':car.id}) }}">   {{car.model}}</a>
                     </h5>
                     <p class="card-text">{{car.carburant}}</p>
                     <div class="text-primary">{{car.price}}DA</div>
                   </div>
               </div>         
            </div>
             {% endfor %}
     </div>
     </div>   
    
    
 {% endblock %}
  
 